{
  "name": "start-simple",
  "version": "1.0.6",
  "description": "A simple one page template for Startups and Small Business built with Bootstrap 4 by TemplateFlip.com",
  "scripts": {
    "clean": "mmpilot clean",
    "build": "mmpilot build",
    "serve": "mmpilot serve",
    "dev-build": "mmpilot build -d",
    "dev-watch": "mmpilot watch -d",
    "dev": "npm-run-all clean dev-build --parallel dev-watch serve",
    "prod": "npm-run-all clean build serve",
    "deploy": "npm-run-all clean build && mmpilot deploy",
    "dist": "npm-run-all clean dev-build && node dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/templateflip/start-simple.git"
  },
  "author": "TemplateFlip.com",
  "license": "Creative Commons Attribution 3.0 Unported License",
  "bugs": {
    "url": "https://github.com/templateflip/start-simple/issues"
  },
  "homepage": "https://github.com/templateflip/start-simple#readme",
  "devDependencies": {
    "mmpilot": "^0.6.6",
    "npm-run-all": "^4.1.2",
    "shelljs": "^0.8.1",
    "node-sass": "^4.7.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0"
  }
}
